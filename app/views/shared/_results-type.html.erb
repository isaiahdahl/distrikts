<div id="results">

<div class="results-container">

  <div class="results-left text-center">
    <%= image_tag "compass.png", class: "large-icon" %>
    <div class="message-name">

      <h3 class="bold text-center"><%= @styles.first.name %></h3>
      <div class="retake">
        <a class="text-link" data-toggle="modal" data-target="#modalQuiz">
          <h5 class="text-center retake-right">Retake Quiz</h5>
        </a>
      </div>

    </div>
  </div>

  <div class="results-right">
    <h4 class="light"><%= @styles.first.description %></h4>
  </div>

</div>
</div>
