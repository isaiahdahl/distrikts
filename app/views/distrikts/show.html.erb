<% content_for(:title_tag) do %>
  Distrikts: Welcome to <%= @distrikt.name %>!
<% end %>

<div class="distrik-container">
  <%= render 'shared/distrikt-banner' %>
  <%= render 'shared/show-nav' %>

  <div class="map-places-container" id="map-container">
    <div class="map-left" id="map-left">
      <%= render 'shared/map' %>
    </div>
    <div class="places-right" id="places-right">
      <%= render 'shared/places-index' %>
    </div>
  </div>
</div>

<div id="instagram-block">
  <%= render 'shared/instagram-block' %>
</div>

<div id="show-cards">
<%= render "shared/show-cards" %>
</div>

<%= render 'shared/place-modal' %>

